{
  "name": "rocketchat",
  "version": "0.0.1",
  "description": "Test task for RB",
  "scripts": {
    "dev": "NODE_ENV=development node ./webpack/server",
    "build": "rm -rf ./build && NODE_ENV=production webpack --progress --optimize-minimize",
    "clean": "rm -rf ./build ./npm-debug.log ./yarn-error.log ./coverage",
    "test": "BABEL_ENV=test jest --bail --colors --coverage",
    "test:report": "http-server ./coverage/lcov-report/ -o",
    "server": "http-server ./build -go --push-state",
    "lint:style": "stylelint './app/scripts/**/styles.js'",
    "lint:scripts": "eslint --ext .jsx,.js ./app/scripts/**",
    "lint:all": "npm run lint:style && npm run lint:scripts",
    "prepush": "npm run lint:all"
  },
  "engines": {
    "node": ">=7.0.0"
  },
  "jest": {
    "notify": true,
    "verbose": true,
    "rootDir": "app/scripts"
  },
  "author": {
    "name": "Alena Dzhukich",
    "email": "LinSol.0@ya.ru",
    "url": "https://github.com/wop-wops"
  },
  "main": "webpack.config.js",
  "license": "MIT",
